{
    "extends": "@tsconfig/ember/tsconfig.json",
    "compilerOptions": {
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": "./",
        "typeRoots": [
            "./"
        ],
        "paths": {
            "ember-data/*": ["./*"],
            "@ember-data/*": ["./*"],
        },
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true,
        "experimentalDecorators": true
    },
    "include": [
        "types/**/*"
    ],
    "files": [
        "index.d.ts",
        "test/adapter.ts",
        "test/belongs-to.ts",
        "test/error.ts",
        "test/has-many.ts",
        "test/injections.ts",
        "test/lib/assert.ts",
        "test/model.ts",
        "test/module-api.ts",
        "test/record-reference.ts",
        "test/relationships.ts",
        "test/request.ts",
        "test/serializer.ts",
        "test/store.ts",
        "test/transform.ts"
    ]
}
